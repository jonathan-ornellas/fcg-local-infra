{
  "ReverseProxy": {
    "Routes": {
      "users": {
        "ClusterId": "users",
        "Match": {
          "Path": "/users/{**catch-all}"
        }
      },
      "auth": {
        "ClusterId": "users",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      },
      "admin": {
        "ClusterId": "users",
        "Match": {
          "Path": "/admin/{**catch-all}"
        }
      },
      "games": {
        "ClusterId": "games",
        "Match": {
          "Path": "/games/{**catch-all}"
        }
      },
      "search": {
        "ClusterId": "games",
        "Match": {
          "Path": "/search/{**catch-all}"
        }
      },
      "reco": {
        "ClusterId": "games",
        "Match": {
          "Path": "/recommendations{**catch-all}"
        }
      },
      "payments": {
        "ClusterId": "payments",
        "Match": {
          "Path": "/payments/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "users": {
        "Destinations": {
          "d1": {
            "Address": "http://fcg-users-api:8080/"
          }
        }
      },
      "games": {
        "Destinations": {
          "d1": {
            "Address": "http://fcg-games-api:8080/"
          }
        }
      },
      "payments": {
        "Destinations": {
          "d1": {
            "Address": "http://fcg-payments-api:8080/"
          }
        }
      }
    }
  },
  "OpenTelemetry": {
    "OtlpEndpoint": "http://jaeger:4317"
  }
}